{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            "single"
        ],
        "max-classes-per-file": [
            false
        ],
        "no-var-requires": false,
        "indent": [
            false
        ],
        "object-literal-sort-keys": false,
        "object-literal-key-quotes": [
            false
        ]
    },
    "rulesDirectory": []
}